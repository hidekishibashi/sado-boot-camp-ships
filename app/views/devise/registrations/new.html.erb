      <script>
        const Displacement = {
            "1": ["3m未満", "3m以上4m未満", "4m以上5m未満", "5m以上6m未満"],
            "2": ["3m以上4m未満", "4m以上5m未満", "5m以上6m未満"],
            "3": ["3m未満", "3m以上4ｍ未満"],
            "4": ["6m以上7m未満", "7m以上8m未満", "8m以上9m未満"],
            "5": ["自動車", "原付（125cc以下）", "750cc未満", "750cc以上"]
         };
  
        function setMenuOptions(selectedCarModel){
          const selectCarDisplacement = document.getElementById('car_displacement'); 
  
          Displacement[selectedCarModel].forEach((displacement, index) => {
            const option = document.createElement('option'); 
            console.log(option);
            option.value = index; 
            option.innerHTML = displacement; 
            selectCarDisplacement.appendChild(option); 
          });
          
        };
  
        const modelSelect = document.getElementById('car_model');
        
        modelSelect.addEventListener('change', (e) => {
            setMenuOptions(e.target.value); 
        });
      </script>
