<p id="notice"><%= notice %></p>

<h1>M6 Ms</h1>

<table>
  <thead>
    <tr>
      <th>Text</th>
      <th>Level</th>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @m6_ms.each do |m6_m| %>
      <tr>
        <td><%= m6_m.text %></td>
        <td><%= m6_m.level %></td>
        <td><%= m6_m.type %></td>
        <td><%= link_to 'Show', m6_m %></td>
        <td><%= link_to 'Edit', edit_m6_m_path(m6_m) %></td>
        <td><%= link_to 'Destroy', m6_m, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

